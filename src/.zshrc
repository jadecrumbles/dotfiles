# Pre-Set
export ZSH="$HOME/.oh-my-zsh"
ZSH_THEME="darkblood"
ENABLE_CORRECTION="true"
HIST_STAMPS="yyyy-mm-dd"
plugins=(git)
source $ZSH/oh-my-zsh.sh

# User-Set
alias ls='eza'
alias du='dust'
alias github='cd ~/Documents/GitHub'
alias neofetch='fastfetch'
alias nano='micro'
alias gamma='xrandr --output eDP-1 --gamma'
